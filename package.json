    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@faker-js/faker": "^7.6.0",
    "@react-pdf/renderer": "^3.1.9",
    "@storybook/addon-actions": "^7.0.6",
    "@storybook/addon-essentials": "^7.0.6",
    "@storybook/addon-postcss": "^2.0.0",
